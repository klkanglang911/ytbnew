# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=password
REDIS_DB=0

# 应用配置
DEBUG=false
LOG_LEVEL=INFO
LOG_FORMAT=text

# 缓存配置
CACHE_TTL=21600

# yt-dlp 配置
YTDLP_TIMEOUT=30
YTDLP_MAX_RETRIES=3

# 并发控制
MAX_CONCURRENT_STREAMS=10
MAX_CONCURRENT_YTDLP_REQUESTS=3

# 请求超时
REQUEST_TIMEOUT=30

# Grafana 管理员密码
GRAFANA_PASSWORD=admin

# WeChat 企业号 Webhook URL（告警通知）
WEBHOOK_URL=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=YOUR_KEY_HERE
